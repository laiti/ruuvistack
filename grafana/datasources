apiVersion: 1
datasources:
- name: InfluxDB
  type: influxdb
  access: proxy
  orgId: 1
  url: http://influxdb:8086
  jsonData:
    version: Flux
    organization: ${INFLUXDB_ORG}
    defaultBucket: ${INFLUXDB_BUCKET}
    httpMode: POST
    tlsSkipVerify: false
    tlsAuth: false
    tlsAuthWithCACert: false
    minTimeInterval: 10s
    maxSeries: 1000
  secureJsonData:
    token: ${INFLUXDB_USER_TOKEN}
  basicAuth: false
